{
    "compilerOptions": {
      "target": "ES2020",
      "module": "CommonJS",
      "moduleResolution": "node",
      "outDir": "out",
      "rootDir": "src",
      "strict": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "sourceMap": true,
      "resolveJsonModule": true,
      
    },
    "exclude": ["node_modules", ".vscode-test"],
    "include": ["src", "src/test"]
  }
  